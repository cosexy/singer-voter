    <div class="content-body">
        <!-- row -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-12">
                    <div class="table-responsive table-hover fs-14">
                        <table class="table display mb-4 dataTablesCard  card-table" id="example5">
                            <thead>
                            <tr>
                                <th>Name</th>
                                <th>Vote</th>
                                <th>Created At</th>
                                <th>Updated At</th>
                                <th style="text-align: right">Action</th>
                            </tr>
                            </thead>
                            <tbody>
                            <% actors.forEach(function(actor){ %>
                                <tr row-id='<%- actor.id %>'>
                                    <td>
                                        <div class="d-flex align-items-center">
                                            <img src="<%- actor.avatar %>"  class="rounded-circle mr-2 width40 height40" alt="">
                                            <span><%- actor.name %></span>
                                        </div>
                                    </td>
                                    <td class="wspace-no _count_likes">
                                        <%- actor.likes %>
                                    </td>
                                    <td>
                                        <span class="text-success font-w700">
                                            <%- actor.createdAt %>
                                        </span>
                                    </td>
                                    <td>
                                        <p class="mb-0 wspace-no">
                                            <%- actor.updatedAt %>
                                        </p>
                                    </td>
                                    <td class="d-flex justify-content-end">
                                        <button type="button" class="btn btn-sm btn-danger _delete" data-id="<%- actor.id %>">
                                            Detele
                                        </button>
                                        <button type="button" class="btn btn-sm btn-primary ml-2 _edit" data-id="<%- actor.id %>">
                                            Edit
                                        </button>
                                    </td>
                                </tr>
                            <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
